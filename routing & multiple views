angular.
  module('phonecatApp').
  config(['$routeProvider',
    function config($routeProvider) {
      $routeProvider.
        when('/phones', {
          template: '<phone-list></phone-list>'
        }).
        when('/phones/:phoneId', {
          template: '<phone-detail></phone-detail>'
        }).
        when('/page-no-found', {
          template: '<p>404 - Page Not Found</p>'
        }).
        otherwise('/page-no-found');
    }
  ]);
